		<constant name="BLOCKED" value="2" enum="Enum">
		</constant>
		<constant name="IDLE" value="3" enum="Enum">
		</constant>
		<constant name="DND" value="4" enum="Enum">
		</constant>
		<constant name="INVISIBLE" value="5" enum="Enum">
		</constant>
		<constant name="STREAMING" value="6" enum="Enum">
		</constant>
		<constant name="UNKNOWN" value="7" enum="Enum">
